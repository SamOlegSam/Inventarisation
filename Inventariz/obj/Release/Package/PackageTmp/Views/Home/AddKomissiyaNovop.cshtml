@model Inventariz.Models.Podpisanty

<div class="modal-header" style="text-align: center;background-color: #D0EDFE; border-radius:5px;">
    <h5> Добавление члена комиссии ЛПДС Полоцк</h5>
</div>
<div class="modal-body">       

    <label>Назначение</label>
    <select id="NaznN" class="form-control btn-block">
        <option>Председатель комиссии</option>
        <option>Член комиссии</option>
        <option>МОЛ</option>
    </select>

    <label>Должность</label>
    <input type="text" class="form-control btn-block" id="DoljnostN" />

    <label>ФИО</label>
    <input type="text" class="form-control btn-block" id="FION" />
</div>
<div class="modal-footer" style="text-align:center">
    <button type="submit" class="btn btn-info" style="min-width:100px" onclick="KomissiyaSaveNovop(0)">Сохранить</button>
    <button type="button" class="btn btn-default" data-dismiss="modal" style="min-width:100px">Отмена</button>
</div>


