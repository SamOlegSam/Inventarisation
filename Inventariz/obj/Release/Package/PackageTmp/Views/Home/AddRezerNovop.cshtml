@model Inventariz.Models.trl_tank
<div class="modal-header" style="text-align: center;background-color: #D0EDFE; border-radius:5px;">
    <h5> Добавление нового резервуара ЛПДС Полоцк</h5>
</div>
<div class="modal-body">

    <label>Номер резервуара</label>
    <input type="text" class="form-control btn-block" id="tankidNovop" />

    <label>Наименование</label>
    <input type="text" class="form-control btn-block" id="NaimenovanRezerNovop" />

    <label>Тип</label>
    @Html.DropDownList("typeMoz", ViewBag.typeNovop as SelectList, "Выберите тип", new { @class = "form-control btn-block", id = "typeNovop" })

    <label>Минимально допустимый уровень</label>
    <input type="text" class="form-control btn-block" id="UrovMinNovop" onkeyup="this.value = this.value.replace(/[^(0-9)(,)]/,'');" />

    <label>Минимально допустимый Объём</label>
    <input type="text" class="form-control btn-block" id="VminNovop" onkeyup="this.value = this.value.replace(/[^(0-9)(,)]/,'');" />

</div>
<div class="modal-footer" style="text-align:center">
    <button type="submit" class="btn btn-info" style="min-width:100px" onclick="RezerNovopSave(0)">Сохранить</button>
    <button type="button" class="btn btn-default" data-dismiss="modal" style="min-width:100px">Отмена</button>
</div>




