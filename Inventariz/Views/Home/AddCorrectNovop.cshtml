@model Inventariz.Models.Correct
<div class="modal-header" style="text-align: center;background-color: #D0EDFE; border-radius:5px;">
    <h5> Добавление корректировки ЛПДС Полоцк</h5>
</div>
<div class="modal-body">

    <label>Резервуар</label>

    @Html.DropDownList("name", ViewBag.corNovop as SelectList, "Выберите резервуар", new { @class = "form-control btn-block", id = "tankidNovop" })

    <label>Уровень</label>
    <input type="text" class="form-control btn-block" id="UrovCNovop" />

    <label>Температура</label>
    <input type="text" class="form-control btn-block" id="TempCNovop" />
</div>
<div class="modal-footer" style="text-align:center">
    <button type="submit" class="btn btn-info" style="min-width:100px" onclick="CorrectNovopSave(0)">Сохранить</button>
    <button type="button" class="btn btn-default" data-dismiss="modal" style="min-width:100px">Отмена</button>
</div>




